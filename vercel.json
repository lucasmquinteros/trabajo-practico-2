{
  "rewrites": [
    { "source": "/character", "destination": "/src/pages/character.html" },
    { "source": "/episode", "destination": "/src/pages/episode.html" },
    { "source": "/episodes", "destination": "/src/pages/episodes.html" },
    { "source": "/location", "destination": "/src/pages/location.html" },
    { "source": "/locations", "destination": "/src/pages/locations.html" }
  ],
  "routes": [
    { "handle": "filesystem" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
